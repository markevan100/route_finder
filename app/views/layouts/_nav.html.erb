<div class="navbar navbar-default navbar-static-top">
  <div class="row">
    <div class="col-xs-4" style="margin-top: 29px;">
      <% if current_user %>
        <span class="glyphicon glyphicon-user"></span>
        <%= current_user.email %>
      <% end %>
    </div>
    <div class="col-xs-4 text-center">
      <h1> <%= link_to "Hit The Proj?", root_path, id: "thenav" %></h1>
    </div>
    <div class="col-xs-4 text-right" style="margin-top: 29px;">
        <% if current_user %>
          <%= link_to("logout", destroy_user_session_path, class: "btn btn-danger", method: :delete) %>
        <% else %>
          <%= link_to("login", new_user_session_path, class: "btn btn-primary") %>
          <%= link_to("sign up", new_user_registration_path, class: "btn btn-success") %>
        <% end %>
    </div>
  </div>
</div>
