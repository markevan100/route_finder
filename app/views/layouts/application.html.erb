<!DOCTYPE html>
<html>
  <head>
    <title>RouteFinder</title>
    <%= Gon::Base.render_data %>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body id='home-body'>
    <%= render 'layouts/nav' %>
    <div class="container">
    <%= yield %>
  </body>
</html>
